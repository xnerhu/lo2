{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "A template for building minified and compressed isomorphic React",
  "homepage": "https://github.com/xnerhu/react-ssr#readme",
  "author": "Miko≈Çaj Palkiewicz <xnerhu@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/dev.js",
    "build-client": "webpack --config webpack.config.client.js",
    "build-server": "webpack --config webpack.config.server.js",
    "build": "cross-env NODE_ENV=production concurrently \"npm run clean\" \"npm run build-client\" \"npm run build-server\"",
    "stats": "cross-env NODE_ENV=production BUNDLE_STATS=enabled concurrently \"mkdir build\" \"webpack --profile --json --config webpack.config.client.js > build/stats.json\"",
    "start": "node ./build/server/main.js",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "clean": "rimraf ./build"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@loadable/babel-plugin": "^5.10.3",
    "@loadable/webpack-plugin": "^5.7.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/nodemon": "^1.19.0",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "4.4.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "6.0.3",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.3.0",
    "file-type": "12.4.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "nodemon": "^2.0.1",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "terser": "^4.4.0",
    "terser-webpack-plugin": "^2.2.1",
    "typescript": "3.7.2",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/component": "^5.10.3",
    "@loadable/server": "^5.10.3",
    "axios": "^0.19.0",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "mobx": "^5.15.0",
    "mobx-react-lite": "^1.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sql-next": "^1.2.1",
    "styled-components": "^5.0.0-beta.8-groupsizefix"
  }
}
