{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "A template for building minified and compressed isomorphic React",
  "homepage": "https://github.com/xnerhu/react-ssr#readme",
  "author": "Miko≈Çaj Palkiewicz <xnerhu@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/dev.js",
    "build-client": "webpack --config webpack.config.client.js",
    "build-server": "webpack --config webpack.config.server.js",
    "build": "cross-env NODE_ENV=production concurrently \"npm run clean\" \"npm run build-client\" \"npm run build-server\"",
    "stats": "cross-env NODE_ENV=production BUNDLE_STATS=enabled concurrently \"mkdir build\" \"webpack --profile --json --config webpack.config.client.js > build/stats.json\"",
    "start": "node ./build/server/main.js",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "clean": "rimraf ./build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/knex": "^0.16.1",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/nodemon": "^1.19.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "4.4.2",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-hot-middleware": "^2.25.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "file-type": "13.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "terser": "^4.6.3",
    "terser-webpack-plugin": "^2.3.2",
    "typescript": "3.7.5",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "axios": "^0.19.1",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "knex": "^0.20.8",
    "mobx": "^5.15.2",
    "mobx-react-lite": "^1.5.2",
    "mysql": "^2.18.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0"
  }
}
